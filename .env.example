# Ejemplo de .env para publicar (placeholders). Rellena con tus valores reales en tu .env local.
# NO subir el .env real con secretos al repositorio. Añade .env a .gitignore.

# General
TIMEZONE=Europe/Madrid
EMAIL=tu@correo.com
PASSWORD=changeme                # contraseña usada por nginx/mariadb
PASSWORD_HASH=changeme_hash

# DuckDNS
DUCKDNS_USER=1000
DUCKDNS_GROUP=1000
DUCKDNS_FOLDER=/path/to/duckdns
DOMINIO=mi-subdominio            
TOKEN_DUCKDNS=your-duckdns-token

# Portainer
PORTAINER_FOLDER=/path/to/portainer

# Jellyfin
JELLYFIN_USER=1000
JELLYFIN_GROUP=1000
JELLYFIN_CONFIG=/path/to/jellyfin/config
JELLYFIN_CACHE=/path/to/jellyfin/cache

# Media
MEDIA=/path/to/media

# Nginx Proxy Manager (NPM)
DNS_NGINX=1.1.1.1
NGINX_CONFIG=/path/to/nginx/config
NGINX_DB_USER=npmuser
NGINX_DB_FOLDER=/path/to/nginx/db

# Authelia
AUTHELIA_FOLDER=/path/to/authelia

# Sonarr / Radarr
UID_SONARR=1000
PGID_SONARR=1000
SONARR_FOLDER=/path/to/sonarr

UID_RADARR=1000
PGID_RADARR=1000
RADARR_FOLDER=/path/to/radarr

# Jellyseerr
UID_JELLYSEERR=1000
PGID_JELLYSEERR=1000
JELLYSEERR_FOLDER=/path/to/jellyseerr

# qBittorrent
UID_QBIT=1000
PGID_QBIT=1000
QBIT_FOLDER=/path/to/qbittorrent

# Tor proxy
TORPROXY_PASSWORD=changeme

# Jackett
UID_JACKETT=1000
GID_JACKETT=1000
JACKETT_FOLDER=/path/to/jackett

# Pi-hole
UID_PIHOLE=1000
PGID_PIHOLE=1000
PASSWORD_PIHOLE=changeme
PIHOLE_FOLDER=/path/to/pihole

# Speedtest Tracker
UID_SPEEDTEST=1000
PGID_SPEEDTEST=1000
SPEEDTEST_FOLDER=/path/to/speedtest
URL_SPEEDTEST=https://tu.dominio/speedtest
SPEEDTEST_KEY=changeme
PASSWORD_SPEEDTEST=changeme
SERVIDOR_DNS=1.1.1.1
TELEGRAM_TOKEN=123456:ABC-DEF1234ghIkl-zyx57W2v1u123ew11
TELEGRAM_CHATID=123456789

# WireGuard (wg-easy)
WG_DNS=1.1.1.1
WG_HOST=tu.dominio.com
WG_FOLDER=/path/to/wg
# WG_PORT se configura en compose (por defecto 51820)

# RustDesk
RD_FOLDER=/path/to/rustdesk
